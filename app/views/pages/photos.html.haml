%nav{"aria-label" => "breadcrumb"}
  %ol.breadcrumb
    %li.breadcrumb-item= link_to "Home", landing_pages_path
    %li.breadcrumb-item= link_to "Albums", albums_path
    %li.breadcrumb-item.active{"aria-current" => "page"} Photos

.container
  - @photos.each do |photo|
    .col-lg-6.col-md-6.col-sm-6.col-xs-12
      .panel.panel-default
        .panel-heading
          = image_tag(photo, width: 600, class: "img-responsive img-thumbnail")
